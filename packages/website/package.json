{
    "name": "@tiro-website/website",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "export": "next build && next export",
        "clean": "rimraf .next out",
        "prettier": "yarn g:prettier \"./**/*.{js,jsx,ts,tsx,html,md,mdx,json}\" --write --config $PROJECT_CWD/.prettierrc --ignore-path $PROJECT_CWD/.prettierignore",
        "lint": "yarn g:eslint \"src\" --ext js,jsx,ts,tsx",
        "check-types": "tsc --noEmit",
        "update-monaco-assets": "rimraf public/monaco && mkdir -p public/monaco && cp -r $PROJECT_CWD/node_modules/monaco-editor/min/* public/monaco/"
    },
    "devDependencies": {
        "@blueprintjs/core": "^3.42.0",
        "@monaco-editor/react": "^4.1.3",
        "@svgr/webpack": "^5.5.0",
        "@tiro-website/wasm_sandbox": "workspace:*",
        "@types/emscripten": "^1.39.4",
        "@types/node": "^14.14.37",
        "@types/react": "^17.0.3",
        "@types/react-inspector": "^4.0.1",
        "eslint-config-next": "^11.0.1",
        "fast-glob": "^3.2.7",
        "file-loader": "^6.2.0",
        "gray-matter": "^4.0.3",
        "immutable": "^4.0.0-rc.14",
        "monaco-editor": "^0.26.1",
        "next": "^11.0.1",
        "next-mdx-remote": "^3.0.4",
        "next-plugin-preval": "^1.2.1",
        "prism-react-renderer": "^1.2.1",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-inspector": "^5.1.1",
        "react-split": "^2.0.11",
        "rehype-autolink-headings": "^5.1.0",
        "remark-heading-id": "^1.0.0",
        "remark-slug": "^6.1.0",
        "sass": "1.32.13",
        "stringify-object": "^3.3.0",
        "typescript": "^4.3.5",
        "webpack": "^5.47.1",
        "webpack-bundle-analyzer": "^4.4.2"
    }
}
